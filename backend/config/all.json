{
  "app": {
    "name": "MEAN boilerplate",
    "logRequests": false,
    "tokenExpiryTimeInMinutes": 43200
  },
  "server": {
    "host": "127.0.0.1",
    "port": 3007
  },
  "i18n": {
    "locales": ["en", "de"],
    "defaultLocale": "en",
    "directory": "%rootPath%/backend/locales",
    "extension": ".json"
  },
  "agenda": {
    "collection": "agendaJobs",
    "maxConcurrency": 20,
    "defaultConcurrency": 1,
    "processEvery": "1 second"
  }
}

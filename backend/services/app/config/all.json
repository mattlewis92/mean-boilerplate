{
  "app": {
    "name": "Nasty boilerplate",
    "logRequests": false,
    "tokenExpiryTime": "30 days"
  },
  "server": {
    "host": "127.0.0.1",
    "port": 3008
  },
  "i18n": {
    "locales": {
      "en": "English",
      "de": "German"
    },
    "defaultLocale": "en",
    "directory": "{rootPath}/backend/services/app/i18n/translations",
    "extension": ".json"
  },
  "kue": {
    "adminPort": 3001,
    "options": {
      "prefix": "q"
    }
  },
  "mailer": {
    "service": "Gmail",
    "auth": {
      "user": "matt@iwaz.at",
      "pass": "<<DELETED>>"
    },
    "defaultSender": "Your app <no-reply@your-app.tld>",
    "sandbox": "matt@iwaz.at"
  },
  "inbox": {
    "gmail": {
      "port": false,
      "host": "imap.gmail.com",
      "options": {
        "secureConnection": true,
        "auth": {
          "user": "matt@iwaz.at",
          "pass": "<<DELETED>>"
        }
      }
    }
  },
  "redis": {
    "host": "127.0.0.1",
    "port": 6379
  }

{
  "name": "mean-boilerplate",
  "version": "0.0.1",
  "homepage": "https://github.com/mattlewis92/mean-boilerplate",
  "authors": [
    "Matt Lewis <matt@iwaz.at>"
  ],
  "description": "Yet another MEAN boilerplate",
  "moduleType": [
    "globals",
    "node"
  ],
  "license": "MIT",
  "private": true,
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "angular": "~1.2.21",
    "bootstrap": "~3.2",
    "angular-bootstrap": "~0.11.0",
    "angular-animate": "~1.2.21",
    "angular-ui-router": "~0.2.10",
    "angular-touch": "~1.2.21",
    "angular-classy": "~0.4.2",
    "restangular": "~1.4.0",
    "angular-cache": "~3.1.0",
    "font-awesome": "~4.1.0",
    "angular-auto-validate": "~1.0.15",
    "angular-translate": "~2.2.0",
    "exceptions.js": "https://github.com/steaks/exceptions.js.git#~0.2.0",
    "angular-promise-tracker": "~2.0.1",
    "fingerprint": "~0.5.4",
    "ngstorage": "~0.3.0",
    "angular-data": "~0.10.5"
  },
  "overrides": {
    "bootstrap": {
      "ignore": true
    },
    "exceptions.js": {
      "main": "exceptions.js"
    },
    "angular-promise-tracker": {
      "main": [
        "promise-tracker.js",
        "promise-tracker-http-interceptor.js"
      ]
    }
  },
  "resolutions": {
    "angular": "1.2.21"
  }
}

{
  "name": "<%= _.slugify(appName) %>",
  "version": "0.0.1",
  "description": "<%= appDescription %>",
  "moduleType": [
    "globals",
    "node"
  ],
  "license": "MIT",
  "private": true,
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "angular": "~1.3.0",
    "bootstrap": "~3.3",
    "angular-bootstrap": "~0.12.0",
    "angular-animate": "~1.3.0",
    "angular-ui-router": "~0.2.13",
    "angular-touch": "~1.3.0",
    "angular-classy": "~1.0.0",
    "font-awesome": "~4.2.0",
    "angular-auto-validate": "~1.11.21",
    "angular-translate": "~2.4.0",
    "exceptions.js": "https://github.com/steaks/exceptions.js.git#~0.3.1",
    "angular-promise-tracker": "~2.1.0",
    "fingerprint": "~0.5.4",
    "ngstorage": "~0.3.0",
    "angular-data": "~1.4.0",
    "angular-cache": "~3.2.0",
    "angular-translate-loader-url": "~2.4.0",
    "angular-translate-handler-log": "~2.4.0",
    "socket.io-client": "~1.2.0",
    "angular-socket-io": "~0.6.0",
    "ui-router-extras": "~0.0.10",
    "angular-bootstrap-checkbox": "~0.3.1",
    "angular-file-upload": "~1.1.5",
    "bootstrap-social": "~4.8.0",
    "angular-ui-select": "~0.8.3",
    "angular-sanitize": "~1.3.0",
    "angular-bluebird-promises": "~0.2.0"
  },
  "overrides": {
    "bootstrap": {
      "ignore": true
    },
    "bootstrap-social": {
      "ignore": true
    },
    "exceptions.js": {
      "main": "exceptions.js"
    },
    "angular-promise-tracker": {
      "main": [
        "promise-tracker.js",
        "promise-tracker-http-interceptor.js"
      ]
    },
    "socket.io-client": {
      "main": "socket.io.js"
    },
    "angular-file-upload": {
      "dependencies": null
    }
  },
  "resolutions": {
    "angular": "~1.3.0"
  }
}
